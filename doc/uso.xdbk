		</para>
	</sect3>

	<sect3 id="anexos">
		<title>Anexos</title>


  <informalfigure>
	<screenshot><mediaobject><imageobject>
	<imagedata fileref="pfichaanexos.eps" format="EPS" scale="40" align="center"></imagedata></imageobject>
	<imageobject><imagedata fileref="pfichaanexos.png" format="PNG" align="center"></imagedata></imageobject>
	<textobject><phrase>Anexos</phrase></textobject></mediaobject>
